<%- include('../partials/header') %>
<table id="list">
    <thead>
      <tr>
        <th>Airline</th>
        <th>Airport</th>
        <th>FlightNo</th>
        <th>Departs</th>
      </tr>
    </thead>
   <tbody>
   <% flights.forEach(function(m) => { %>
    <tr>
        <td><%= m.company %></td>
        <td><%= m.state %></td>
        <td><%= m.number %></td>
        <td><%= m.date %></td>
        <td><a href="/flights/<%= m._id %>">DETAILS</a></td>
    </tr>
   <% }); %>
   </tbody>
   </table>
   <% include('../partials/footer') %> 